{
    "name": "kickstand-ui",
    "version": "0.4.1",
    "description": "The Customizable Design System",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/kickstand-ui/kickstand-ui.js",
    "files": [
        "dist/",
        "loader/",
        "src/scss/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "dev": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test:watch": "stencil test --spec --e2e --watchAll",
        "deploy": "stencil build && publish",
        "generate": "stencil generate",
        "start": "stencil build --watch",
        "storybook": "start-storybook -p 9009 -s www",
        "storybook:watch": "run-p start storybook",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:deploy": "./deploy-docs.sh"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@stencil/core": "^1.8.4",
        "@stencil/postcss": "^1.0.1",
        "@stencil/sass": "^1.1.1",
        "@stencil/webpack": "0.0.6",
        "@storybook/addon-a11y": "^5.3.18",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-info": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-viewport": "^5.3.18",
        "@storybook/html": "^5.3.18",
        "@types/autoprefixer": "^9.6.1",
        "@vuepress/plugin-google-analytics": "^1.5.0",
        "autoprefixer": "^9.6.5",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "react-docgen-typescript-loader": "^3.7.2",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "ts-loader": "^7.0.1",
        "vuepress": "^1.5.0"
    },
    "author": "Burton Smith <burton@breakstuff.io>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/break-stuff/kickstand-ui.git"
    },
    "bugs": {
        "url": "https://github.com/break-stuff/kickstand-ui/issues"
    },
    "license": "MIT",
    "keywords": [
        "css",
        "sass",
        "mobile-first",
        "responsive",
        "front-end",
        "framework",
        "web",
        "mobile-first",
        "web components",
        "design system",
        "stenciljs",
        "clarion"
    ]
